{
  "name": "invert-color",
  "version": "1.1.0",
  "description": "Generates inverted (opposite) version of the given color.",
  "main": "dist/invert.js",
  "scripts": {
    "test": "node test",
    "build": "babel src --presets es2015 --plugins transform-es2015-modules-umd --out-file dist/invert.js",
    "watch": "babel -w src  --presets es2015 --plugins transform-es2015-modules-umd --out-file dist/invert.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onury/invert-color.git"
  },
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "color",
    "invert",
    "opposite",
    "generate",
    "hex",
    "rgb"
  ],
  "author": "Onur Yildirim <onur@cutepilot.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onury/invert-color/issues"
  },
  "homepage": "https://github.com/onury/invert-color#readme",
  "devDependencies": {
    "jasmine": "^2.7.0",
    "jasmine-console-reporter": "^2.0.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1"
  }
}
